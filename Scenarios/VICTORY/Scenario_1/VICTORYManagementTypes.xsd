<xsd:schema xmlns:vmt="http://www.victory-standards.org/Schemas/VICTORYManagementTypes.xsd"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:vst="http://www.victory-standards.org/Schemas/VICTORYSharedTypes.xsd"
    targetNamespace="http://www.victory-standards.org/Schemas/VICTORYManagementTypes.xsd"
    elementFormDefault="qualified" version="1.7">
    <xsd:import namespace="http://www.victory-standards.org/Schemas/VICTORYSharedTypes.xsd"
        schemaLocation="VICTORYSharedTypes.xsd"/>
    <!--SIMPLE TYPES-->
    <xsd:simpleType name="dscp_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:byte">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="51"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="fieldOfViewBounds_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:double">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="180"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ipAddress_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:union memberTypes="vmt:ipv4Address_t vmt:ipv6Address_t"/>
    </xsd:simpleType>
    <xsd:simpleType name="ipv4Address_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern
                value="(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)"
            />
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ipv6Address_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern
                value="((([0-9A-Fa-f]{1,4}:){7}(([0-9A-Fa-f]{1,4})|:))|(([0-9A-Fa-f]{1,4}:){6}(:|((25[0-5]|2[0-4]\d|[01]?\d{1,2})(\.(25[0-5]|2[0-4]\d|[01]?\d{1,2})){3})|(:[0-9A-Fa-f]{1,4})))|(([0-9A-Fa-f]{1,4}:){5}((:((25[0-5]|2[0-4]\d|[01]?\d{1,2})(\.(25[0-5]|2[0-4]\d|[01]?\d{1,2})){3})?)|((:[0-9A-Fa-f]{1,4}){1,2})))|(([0-9A-Fa-f]{1,4}:){4}(:[0-9A-Fa-f]{1,4}){0,1}((:((25[0-5]|2[0-4]\d|[01]?\d{1,2})(\.(25[0-5]|2[0-4]\d|[01]?\d{1,2})){3})?)|((:[0-9A-Fa-f]{1,4}){1,2})))|(([0-9A-Fa-f]{1,4}:){3}(:[0-9A-Fa-f]{1,4}){0,2}((:((25[0-5]|2[0-4]\d|[01]?\d{1,2})(\.(25[0-5]|2[0-4]\d|[01]?\d{1,2})){3})?)|((:[0-9A-Fa-f]{1,4}){1,2})))|(([0-9A-Fa-f]{1,4}:){2}(:[0-9A-Fa-f]{1,4}){0,3}((:((25[0-5]|2[0-4]\d|[01]?\d{1,2})(\.(25[0-5]|2[0-4]\d|[01]?\d{1,2})){3})?)|((:[0-9A-Fa-f]{1,4}){1,2})))|(([0-9A-Fa-f]{1,4}:)(:[0-9A-Fa-f]{1,4}){0,4}((:((25[0-5]|2[0-4]\d|[01]?\d{1,2})(\.(25[0-5]|2[0-4]\d|[01]?\d{1,2})){3})?)|((:[0-9A-Fa-f]{1,4}){1,2})))|(:(:[0-9A-Fa-f]{1,4}){0,5}((:((25[0-5]|2[0-4]\d|[01]?\d{1,2})(\.(25[0-5]|2[0-4]\d|[01]?\d{1,2})){3})?)|((:[0-9A-Fa-f]{1,4}){1,2})))|(((25[0-5]|2[0-4]\d|[01]?\d{1,2})(\.(25[0-5]|2[0-4]\d|[01]?\d{1,2})){3})))(%.+)?"
            />
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="maturity_t">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Preliminary"/>
            <xsd:enumeration value="Experimental"/>
            <xsd:enumeration value="Proposed Standard"/>
            <xsd:enumeration value="Draft Standard"/>
            <xsd:enumeration value="VICTORY Standard"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="radioGainBounds_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:minInclusive value="-27"/>
            <xsd:maxInclusive value="27"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="timeSecNsec_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:decimal">
            <xsd:pattern value="([0-9])+(\.([0-9]){1,9})?"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="transportLayerPort_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="65535"/>
        </xsd:restriction>
    </xsd:simpleType>
    <!--COMPLEX TYPES-->
    <xsd:complexType name="generatorNominalData_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="voltageAvailable" type="xsd:boolean"/>
            <xsd:element name="temperatureAvailable" type="xsd:boolean"/>
            <xsd:element name="shaftSpeedAvailable" type="xsd:boolean"/>
            <xsd:element name="utilizationAvailable" type="xsd:boolean"/>
            <xsd:element name="faultsAvailable" type="xsd:boolean"/>
            <xsd:element name="outputEnabledAvailable" type="xsd:boolean"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ambientConditionsNominalData_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="barometricPressureAvailable" type="xsd:boolean"/>
            <xsd:element name="cabInteriorTemperatureAvailable" type="xsd:boolean"/>
            <xsd:element name="ambientAirTemperatureAvailable" type="xsd:boolean"/>
            <xsd:element name="engineAirIntakeTemperatureAvailable" type="xsd:boolean"/>
            <xsd:element name="roadSurfaceTemperatureAvailable" type="xsd:boolean"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="auxiliaryPowerUnitNominalData_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="powerAvailable" type="xsd:boolean"/>
            <xsd:element name="voltageAvailable" type="xsd:boolean"/>
            <xsd:element name="temperatureAvailable" type="xsd:boolean"/>
            <xsd:element name="faultsAvailable" type="xsd:boolean"/>
            <xsd:element name="outputEnabledAvailable" type="xsd:boolean"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="base_t" abstract="true">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="uniqueName" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="channel_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string" minOccurs="0"/>
            <xsd:element name="frequency" type="xsd:unsignedInt" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="directionOfTravel_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="heading" type="vst:headingBounds_t" minOccurs="0"/>
            <xsd:element name="speed" type="vst:unsignedDouble_t" minOccurs="0"/>
            <xsd:element name="rateOfClimb" type="xsd:double" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="directionOfTravelNominalData_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="headingAvailable" type="xsd:boolean"/>
            <xsd:element name="speedAvailable" type="xsd:boolean"/>
            <xsd:element name="rateOfClimbAvailable" type="xsd:boolean"/>
            <xsd:element name="description" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="error_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="ERRNO" type="xsd:string"/>
            <xsd:element name="ERRTXT" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="fault_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="faultId" type="xsd:unsignedInt"/>
            <xsd:element name="origin" type="xsd:string"/>
            <xsd:element name="severity" type="xsd:string"/>
            <xsd:element name="facility" type="xsd:string"/>
            <xsd:element name="category" type="xsd:string" minOccurs="0"/>
            <xsd:element name="suggestedAction" type="xsd:string" minOccurs="0"/>
            <xsd:element name="state" type="xsd:string"/>
            <xsd:element name="acknowledgeable" type="xsd:boolean"/>
            <xsd:element name="message" type="xsd:string"/>
            <xsd:element name="timestamp" type="xsd:dateTime"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="frequencyBand_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="start" type="xsd:double"/>
            <xsd:element name="stop" type="xsd:double"/>
            <xsd:element name="channelWidth" type="xsd:double"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="geographicRegion_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="latitude" type="vst:latitudeBounds_t"/>
            <xsd:element name="longitude" type="vst:longitudeBounds_t"/>
            <xsd:element name="range" type="vst:unsignedDouble_t"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="lowVoltageThresholdsNominalData_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="currentTripThresholdAvailable" type="xsd:boolean"/>
            <xsd:element name="currentOnlineThresholdAvailable" type="xsd:boolean"/>
            <xsd:element name="voltageUpperThresholdAvailable" type="xsd:boolean"/>
            <xsd:element name="voltageLowerThresholdAvailable" type="xsd:boolean"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="missionRecorderComment_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="commentUniqueName" type="xsd:string"/>
            <xsd:element name="commentValue" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="missionRecorderDataStream_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="dataStreamUniqueName" type="xsd:string"/>
            <xsd:element name="type" type="xsd:string"/>
            <xsd:element name="pastDataEnabled" type="xsd:boolean"/>
            <xsd:element name="state" type="xsd:string"/>
            <xsd:element name="recordingTransportConfiguration" type="vmt:transportConfiguration_t"/>
            <xsd:element name="playbackTransportConfigurations">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" name="playbackTransportConfiguration"
                            type="vmt:transportConfiguration_t"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" name="comments">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" name="comment"
                            type="vmt:missionRecorderComment_t"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" name="recordedFiles">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" name="recordedFile"
                            type="vmt:missionRecorderFileInformation_t"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="missionRecorderFileInformation_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="fileName" type="xsd:string"/>
            <xsd:element name="startTime" type="xsd:dateTime"/>
            <xsd:element name="endTime" type="xsd:dateTime"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="missionRecorderPermanentStorage_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="size">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:double">
                        <xsd:minInclusive value="0"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="units" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="dataLoggerStream_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="uniqueName" type="xsd:string"/>
            <xsd:element name="uri" type="xsd:anyURI"/>
            <xsd:element name="streamType" type="vmt:dataLoggerStreamType_t"/>
            <xsd:element name="timeFilter" type="vmt:dataLoggerTimeFilter_t" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="dataLoggerTimeFilter_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="startTime" type="xsd:dateTime" minOccurs="0"/>
            <xsd:element name="endTime" type="xsd:dateTime" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="dataLoggerStreamFilter_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="uniqueName" type="xsd:string" minOccurs="0"/>
            <xsd:element name="uri" type="xsd:anyURI" minOccurs="0"/>
            <xsd:element name="streamType" type="vmt:dataLoggerStreamType_t" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="dataLoggerStreamType_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice>
            <xsd:element name="vdmStream" type="vmt:vdmStream_t"/>
            <xsd:element name="healthStream" type="vmt:healthStream_t"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="vdmStream_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice>
            <xsd:element name="vdmType" type="xsd:string"/>
            <xsd:element name="interfaceId" type="xsd:unsignedInt"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:simpleType name="healthStream_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="syslog|\d?(\.\d+)+"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="dataLoggerSetStreamError">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="uniqueName" type="xsd:string"/>
            <xsd:element minOccurs="0" name="timeFilter" type="vmt:dataLoggerTimeFilter_t"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="networkLayer_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="layerName" type="xsd:string"/>
            <xsd:element name="networkLayer" type="vmt:networkLayer_t" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="orientation_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="roll" type="vst:rollBounds_t" minOccurs="0"/>
            <xsd:element name="pitch" type="vst:pitchBounds_t" minOccurs="0"/>
            <xsd:element name="yaw" type="vst:yawBounds_t" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="orientationNominalData_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="rollAvailable" type="xsd:boolean"/>
            <xsd:element name="pitchAvailable" type="xsd:boolean"/>
            <xsd:element name="yawAvailable" type="xsd:boolean"/>
            <xsd:element name="description" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="orientationRate_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="rollRate" type="xsd:double" minOccurs="0"/>
            <xsd:element name="pitchRate" type="xsd:double" minOccurs="0"/>
            <xsd:element name="yawRate" type="xsd:double" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="platformBatteryId_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="batteryId" type="xsd:string"/>
            <xsd:element name="name" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="platformBatteryNominalData_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="batteryId" type="xsd:string"/>
            <xsd:element name="stateOfChargeAvailable" type="xsd:boolean"/>
            <xsd:element name="stateOfHealthAvailable" type="xsd:boolean"/>
            <xsd:element name="currentAvailable" type="xsd:boolean"/>
            <xsd:element name="lowVoltageAvailable" type="xsd:boolean"/>
            <xsd:element name="temperatureAvailable" type="xsd:boolean"/>
            <xsd:element name="chargeEnabledAvailable" type="xsd:boolean"/>
            <xsd:element name="interconnectedAvailable" type="xsd:boolean"/>
            <xsd:element name="chargeCurrentAvailable" type="xsd:boolean"/>
            <xsd:element name="chargeVoltageAvailable" type="xsd:boolean"/>
            <xsd:element minOccurs="0" name="description" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="platformPowerGenerationNominalData_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="generator" type="vmt:generatorNominalData_t"/>
            <xsd:element minOccurs="0" name="auxiliaryPowerUnit"
                type="vmt:auxiliaryPowerUnitNominalData_t"/>
            <xsd:element minOccurs="0" name="description" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="platformPowerNominalData_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="instantaneousPowerConsumedAvailable" type="xsd:boolean"/>
            <xsd:element name="instantaneousPowerGeneratedAvailable" type="xsd:boolean"/>
            <xsd:element name="systemVoltageAvailable" type="xsd:boolean"/>
            <xsd:element name="totalInstantaneousEnginePowerAvailable" type="xsd:boolean"/>
            <xsd:element name="availableEnginePowerAvailable" type="xsd:boolean"/>
            <xsd:element minOccurs="0" name="description" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="position_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="latitude" type="vst:latitudeBounds_t" minOccurs="0"/>
            <xsd:element name="longitude" type="vst:longitudeBounds_t" minOccurs="0"/>
            <xsd:element name="altitude" type="xsd:double" minOccurs="0"/>
            <xsd:element name="mgrs" type="vst:mgrs_t" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="positionNominalData_t">
        <xsd:annotation>
            <xsd:appinfo>
                <maturity>Proposed Standard</maturity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="latitudeAvailable" type="xsd:boolean"/>
