    <MeasurementDomain>
      <Name>Example Measurement Domain</Name>
      <Description>unused</Description>
      <Measurements>
        <Measurement ID="Meas_CaptureAll">
          <Name>CAPALL</Name>
          <Description>A Measurement to demonstrate how to capture all ARINC 429 messages from a bus</Description>
          <MeasurementID>0x00000004</MeasurementID>
          <MeasurementType>DigitalBus</MeasurementType>
          <MeasurementActive>true</MeasurementActive>
          <ProperName>None</ProperName>
          <DeliveryClass>FlightSafety</DeliveryClass>
          <DataAttributes>
            <SIUnits>Counts</SIUnits>
            <DigitalAttributes>
              <DataLength>
                <ConditionParameter>
                  <ConditionOperation>==</ConditionOperation>
                  <ConditionValueFlex>32</ConditionValueFlex>
                  <SIUnits>Bit</SIUnits>
                </ConditionParameter>
              </DataLength>
              <Endianness>BigEndian</Endianness>
              <DigitalEncoding>UnsignedBinary</DigitalEncoding>
            </DigitalAttributes>
          </DataAttributes>
        </Measurement>
      </Measurements>
      <DataStreams>
        <DataStream ID="ARINC429_Bus_1">
          <Name>ARINC429_Bus_1</Name>
          <Description>ARINC 429 Bus 1 description</Description>
          <ARINC429Messages>
            <ARINC429Message ID="ARINC429_Bus_1_CaptureAll">
              <Name>CaptureAll</Name>
              <Description>Message definition to demonstrate message selection and masks</Description>
              <!-- Omitting the ARINC429 fields describes an "accept-all" scenario -->
              <DataStructure ID="ARINC429_Bus_1_CaptureAll_STRUCT">
                <Name>unused</Name>
                <Description>unused</Description>
                <PackageDataField ID="ARINC429_Bus_1_CaptureAll_Field">
                  <Name>unused</Name>
                  <Description>unused</Description>
                  <FieldLocation ID="ARINC429_Bus_1_CaptureAll_Field_Loc">
                    <FieldOffset>
                      <OffsetValue>
                        <FlexValue>0</FlexValue>
                        <SIUnits>Bit</SIUnits>
                      </OffsetValue>
                    </FieldOffset>
                    <FieldWidth>
                      <FlexValue>32</FlexValue>
                      <SIUnits>Bit</SIUnits>
                    </FieldWidth>
                  </FieldLocation>
                  <FieldEncoding>
                    <Endianness>BigEndian</Endianness>
                    <DigitalEncoding>UnsignedBinary</DigitalEncoding>
                  </FieldEncoding>
                  <FieldRepetitions>0</FieldRepetitions>
                </PackageDataField>
              </DataStructure>
              <DataMap>
                <DataWordToFieldMap>
                  <DataWord>
                    <Name>unused</Name>
                    <Description>unused</Description>
                    <DataWordWidth>
                      <FlexValue>32</FlexValue>
                      <SIUnits>Bit</SIUnits>
                    </DataWordWidth>
                    <Syllable>
                      <Name>unused</Name>
                      <Description>unused</Description>
                      <MeasurementRef IDREF="Meas_CaptureAll"/>
                      <SyllableMask>0xffffffff</SyllableMask>
                      <SyllableWidth>
                        <FlexValue>32</FlexValue>
                        <SIUnits>Bit</SIUnits>
                      </SyllableWidth>
                      <SyllableStartBit>1</SyllableStartBit>
                      <DataWordOffset>0</DataWordOffset>
                    </Syllable>
                  </DataWord>
                  <PackageDataFieldRef IDREF="ARINC429_Bus_1_CaptureAll_Field"/>
                  <TimeOrder>Unspecified</TimeOrder>
                </DataWordToFieldMap>
              </DataMap>
            </ARINC429Message>
          </ARINC429Messages>
        </DataStream>
      </DataStreams>
    </MeasurementDomain>
