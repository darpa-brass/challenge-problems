<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<MDLRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xmlns="http://inetprogram.org/projects/MDL" 
         xmlns:mdl="http://inetprogram.org/projects/MDL" 
